
<!DOCTYPE html>
<html lang="en-US">

<head>
	<meta charset="UTF-8" />
	<%= stylesheet_link_tag "css/bootstrap.min" %>
	<%= stylesheet_link_tag "css/simple-sidebar" %>
	<link href='https://fonts.googleapis.com/css?family=Montserrat:400,700' rel='stylesheet' type='text/css'>
</head>

<body>


	<div id="wrapper" class="active">
		<%= render 'shared/nav' %>

		<div id="page-content-wrapper">
			<div class="page-content inset">
				<div class="row">
					<div class="jumbotron container" style="text-align: center;">
						<%= image_tag("home.jpg", :style=>"width=100%; max-height:auto;") %>
						<br>
						<%= link_to('Get Started', new_user_registration_path, class: "btn btn-primary btn-lg")  %>
						<p>Already a user?   <%= link_to('Login', new_user_session_path)  %></p>
					</div>
				</div>
			</div>
		</div>

	</div>

	<script src="https://ajax.googleapis.com/ajax/libs/jquery/1.11.3/jquery.min.js"></script>
	<script src="../../assets/bootstrap/js/bootstrap.min.js"></script>

	<script>
		$("#menu-toggle").click(function(e) {
			e.preventDefault();
			$("#wrapper").toggleClass("active");
		});
	</script>
</body>
